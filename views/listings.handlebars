<h1 class="display-3">Listings</h1>
<section class="jumbotron text-center">
  <div class="row d-flex justify-content-center">
    {{#each AllListings}}
      <div id={{location}} class="card mb-4 box-shadow" style="width: 20rem; ">
        <img
          src="../public/images/traveling.jpg"
          class="card-img-top"
          alt="..."
        />
        <div class="card-body">
          <a href="/descriptions/{{_id}}"><h2>{{location}}</h2></a>
          <p>{{propertytype}}</p>
        </div>

      </div>
    {{/each}}
  </div>

</section>
<button class="btn btn-light btn-lg"><a href="/logout">Logout</a></button>